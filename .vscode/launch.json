  "version": "0.2.0",
  "configurations": [
    {
      "name": "server",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "devstart"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/server",
      "type": "pwa-node"
    },
    {
      "name": "flutter",
      "type": "dart",
      "request": "launch",
      "flutterMode": "debug",
      "program": "chunshen/lib/main.dart"
    }
  ]
}